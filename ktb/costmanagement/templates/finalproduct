      $(document).ready(function(){

         $(document).on('click','.view',function(){
            var product=this.value;
            $.post("{% url 'costmanagement:getFinal'%}",
            {
               'product':product
            },
            function(data,status){
              // alert(data.product.product);
               // $('#view-productsn').text(data.product.sn);
               // $('#view-productdate').text(data.product.date);
               // $('#view-productname').text(data.product.product);
               // $('#view-packingsize').text(data.product.packingsize);
               // $('#view-bottleperneck').text(data.product.bottlesperpack.toFixed(4));
               // $('#view-litersperpack').text(data.product.litersperpack.toFixed(4));
               // $('#view-totalqty').text(data.product.totalqty.toFixed(4));
               // $('#view-qtyliters').text(data.product.qtyinliters.toFixed(4));
               // $('#view-perlitercost').text(data.product.perlitercost.toFixed(4));
               // $('#view-costpercasepail').text(data.product.costpercase.toFixed(4));
               // $('#view-dkcost').text(data.product.dkcost.toFixed(4));
               // $('#view_bottlepereac').text(data.product.bottlepereac.toFixed(4));
               // $('#view_bottlepercase').text(data.product.bottlepercase.toFixed(4));
               // $('#view_labelpereac').text(data.product.lablepereac.toFixed(4));
               // $('#view_labelpercase').text(data.product.labelpercase.toFixed(4));
               // $('#view_cartons').text(data.product.cartons.toFixed(4));
               // $('#view_dkexprice').text(data.product.dkexprice.toFixed(4));
               // $('#view_kscost').text(data.product.kscost.toFixed(4));
               // $('#view_totalsellingprice').text(data.product.totalsp.toFixed(4));
                $('#view-productsn').text(data.product.sn);
               $('#view-productdate').text(data.product.date);
               $('#view-productname').text(data.product.product);
               $('#view-packingsize').text(data.product.packingsize);
               $('#view-bottleperneck').text(data.product.bottlesperpack.toFixed(4));
               $('#view-litersperpack').text(data.product.litersperpack.toFixed(4));
               $('#view-totalqty').text(data.product.totalqty.toFixed(4));
               $('#view-qtyliters').text(data.product.qtyinliters.toFixed(4));
               $('#view-perlitercost').text(data.product.perlitercost.toFixed(4));
               $('#view-costpercasepail').text(data.product.costpercase.toFixed(4));
               $('#view-dkcost').text(data.product.dkcost.toFixed(4));
               $('#view-priceperbottle').text(data.product.bottlepereac);
               $('#view-bottlepercase').text(data.product.bottlepercase);
               $('#view-priceperbottlecap').text(data.product.bottlecapprice);
               $('#view-bottlecappercase').text(data.product.bottlecappercase);
               $('#view-priceperlabel').text(data.product.lablepereac);
               $('#view_labelpercase').text(data.product.labelpercase);
               $('#view_pricepercarton').text(data.product.cartons);
               $('#view_dkexprice').text(data.product.dkexprice);
               $('#view_kscost').text(data.product.kscost);
               $('#view_totalsellingprice').text(data.product.totalsp);
               $('#view_remarks').text(data.product.remarks);
            });
       });

         $('.excel').click(function(){
          $(this).attr('href',"{% url 'costmanagement:exportFinalProduct'%}");
         });
         $('#view-finaltotalquantity-input').keyup(function(){
            var netqty=this.value;
            // alert(netqty);
             $.post("{% url 'costmanagement:getFinalProduct'%}",
            {
               'product':$('#view-finalproduct-input').val(),
               'totalQty':netqty,

            },
            function(data,status){
               $('#view-finalproduct-input').val(data.product);
               // $('#view-isograde-input').val(data.product.sae);
               $('#view-finaltotalquantity-input').val(data.totalQty);
               $('#view-final_perlitercost-input').val(data.perLiterCost.toFixed(4));
               // alert(data.perLiterCost);
               // $('#view-final_blendingcost-input').val(data.product.costBlendDk);
               // $('#view-finaltotal_cost-input').val(data.product.totalCost);
               // $('#view-final_cost200liter-input').val(data.product.costPer200);
               // $('#view-final_costofdrum-input').val(data.product.costPerDrum);
