          if(pack=='25l'){$('#edit-final_bottles_perpack-input').val('1');$('#edit-final_liters_perpack-input').val('25');}
          if(pack=='20l'){$('#edit-final_bottles_perpack-input').val('1');$('#edit-final_liters_perpack-input').val('20');}
          if(pack=='18k'){$('#edit-final_bottles_perpack-input').val('1');$('#edit-final_liters_perpack-input').val('18');}
          if(pack=='5l'){$('#edit-final_bottles_perpack-input').val('3');$('#edit-final_liters_perpack-input').val('15');}
          if(pack=='5k'){$('#edit-final_bottles_perpack-input').val('3');$('#edit-final_liters_perpack-input').val('15');}
          if(pack=='4l'){$('#edit-final_bottles_perpack-input').val('4');$('#edit-final_liters_perpack-input').val('16');}
          if(pack=='1l'){$('#edit-final_bottles_perpack-input').val('12');$('#edit-final_liters_perpack-input').val('12');}
          if(pack=='1k'){$('#edit-final_bottles_perpack-input').val('12');$('#edit-final_liters_perpack-input').val('12');}
          if(pack=='500ml'){$('#edit-final_bottles_perpack-input').val('24');$('#edit-edit-edit-final_liters_perpack-input').val('12');}
          if(pack=='500gm'){$('#edit-edit-final_bottles_perpack-input').val('24');$('#edit-final_liters_perpack-input').val('12');}
         });

         $(document).on('click','.edit',function(){
          var product=this.id;
            $.post("{% url 'costmanagement:getFinal'%}",
            {
               'product':product
            },
            function(data,status) {
              // alert(data.product.product);
              $('#edit-finalproduct_sn').val(data.product.sn);
              $('#edit-finalproduct_date-input').val(data.product.date);
              $('#edit-finalproductname-input').val(data.product.product);
              $('#edit-final_packingsize-input').val(data.product.packingsize);
              $('#edit-final_bottles_perpack-input').val(data.product.bottlesperpack.toFixed(4));
              $('#edit-final_liters_perpack-input').val(data.product.litersperpack.toFixed(4));
              $('#edit-final_totalquantity-text-input').val(data.product.totalqty.toFixed(4));
              $('#edit-final_totalquantity-input').val(data.product.totalqtyunit);
              $('#edit-final_qtylitrs-input').val(data.product.qtyinliters.toFixed(4));
              $('#edit-final_perlitercost-input').val(data.product.perlitercost.toFixed(4));
              $('#edit-final_costpercasepail-input').val(data.product.costpercase.toFixed(4));
              $('#edit-final_dkcost-input').val(data.product.dkcost.toFixed(4));
              $('#edit-final_bottle-eac-input').val(data.product.bottlepereac);
              $('#edit-final_bottle-case-input').val(data.product.bottlepercase.toFixed(4));
              $('#edit-final_label-eac-input').val(data.product.lablepereac);
              $('#edit-final_price-bottlecap-input').val(data.product.bottlecapprice);
              $('#edit-final_bottlecap_per-case-input').val(data.product.bottlecappercase.toFixed(4));
              $('#edit-final_label-case-input').val(data.product.labelpercase.toFixed(4));
              $('#edit-final_cartons-input').val(data.product.cartons);
              $('#edit-final_dkexPrice-input').val(data.product.dkexprice.toFixed(4));
              $('#edit-final_kscost-input').val(data.product.kscost.toFixed(4));
              $('#edit-final_sellingprice-input').val(data.product.totalsp.toFixed(4));
              $('#edit-final_remarks-input').val(data.product.remarks);
              // alert(data.product.product);
            });
         });

         $('#final_qtylitrs-input').focus(function(){
          var total=parseFloat($('#final_totalquantity-text-input').val());
          var liters=parseFloat($('#final_liters_perpack-input').val());
          this.value=(total*liters).toFixed(4);
         });

         $('#edit-final_qtylitrs-input').focus(function(){
          var total=parseFloat($('#edit-final_totalquantity-text-input').val());
          var liters=parseFloat($('#edit-final_liters_perpack-input').val());
          this.value=(total*liters).toFixed(4);
         });

         $('#final_costpercasepail-input').focus(function(){
          var total=parseFloat($('#final_perlitercost-input').val());
          var liters=parseFloat($('#final_liters_perpack-input').val());
          this.value=(total*liters).toFixed(4);
         });

          $('#edit-final_costpercasepail-input').focus(function(){
          var total=parseFloat($('#edit-final_perlitercost-input').val());
          var liters=parseFloat($('#edit-final_liters_perpack-input').val());
          this.value=(total*liters).toFixed(4);
         });

         $('#final_bottle-case-input').focus(function(){
          if($('#final_bottle-eac-input').val() == "NA"){
            this.value=0;
          }else{
          var total_str=$('#final_bottle-eac-input').val();
          var total=parseFloat(total_str.substring(total_str.lastIndexOf('-')+1));
          var liters=parseFloat($('#final_bottles_perpack-input').val());
          this.value=(total*liters).toFixed(4);
        }
         });

          $('#edit-final_bottle-case-input').focus(function(){
            if($('#edit-final_bottle-eac-input').val() == "NA"){
            this.value=0;
          }else{
          var total_str=$('#edit-final_bottle-eac-input').val();
          var total=parseFloat(total_str.substring(total_str.lastIndexOf('-')+1));
          var liters=parseFloat($('#edit-final_bottles_perpack-input').val());
          this.value=(total*liters).toFixed(4);
        }
         });

         $('#final_label-case-input').focus(function(){
          if($('#final_label-eac-input').val()=="NA"){
            this.value=0;
          }else{
          var total_str=$('#final_label-eac-input').val();
          var total=parseFloat(total_str.substring(total_str.lastIndexOf('-')+1));
          var liters=parseFloat($('#final_bottles_perpack-input').val());
          this.value=(total*liters).toFixed(4);
        }
         });

         $('#edit-final_label-case-input').focus(function(){
          if($('#edit-final_label-eac-input').val()=="NA"){
            this.value=0;
          }else{
            var total_str=$('#edit-final_label-eac-input').val();
            // alert(total_str.substring(total_str.lastIndexOf('-')+1));
          // var total=parseFloat($('#edit-final_label-eac-input').val());
          var total=parseFloat(total_str.substring(total_str.lastIndexOf('-')+1));
          var liters=parseFloat($('#edit-final_bottles_perpack-input').val());
          this.value=(total*liters).toFixed(4);
        }
         });

         $('#final_bottlecap_per-case-input').focus(function(){
          if($('#final_price-bottlecap-input').val() == "NA"){
            this.value=0;
          }else{
          var total_str=$('#final_price-bottlecap-input').val();
          var total=parseFloat(total_str.substring(total_str.lastIndexOf('-')+1));
          var liters=parseFloat($('#final_bottles_perpack-input').val());
          this.value=(total*liters).toFixed(4);
        }
         });

         $('#edit-final_bottlecap_per-case-input').focus(function(){
          if($('#edit-final_price-bottlecap-input').val()=="NA"){
            this.value=0;
          }else{
          var total_str=$('#edit-final_price-bottlecap-input').val();
          var total=parseFloat(total_str.substring(total_str.lastIndexOf('-')+1));
          var liters=parseFloat($('#edit-final_bottles_perpack-input').val());
          this.value=(total*liters).toFixed(4);
        }
         });

          $('#final_sellingprice-input').focus(function(){
          var total=parseFloat($('#final_dkexPrice-input').val());
          var liters=parseFloat($('#final_kscost-input').val());
          // var label=parseFloat($('#final_label-case-input').val());
          // var price=parseFloat($('#final_cartons-input').val());
          // var bottle=parseFloat($('#final_bottle-case-input').val());
          // var cap=parseFloat($('#final_bottlecap_per-case-input').val());
          this.value=(total+liters).toFixed(4);
         });

           $('#edit-final_sellingprice-input').focus(function(){
          var total=parseFloat($('#edit-final_dkexPrice-input').val());
          var liters=parseFloat($('#edit-final_kscost-input').val());
          // var label=parseFloat($('#edit-final_label-case-input').val());
          // var price=parseFloat($('#edit-final_cartons-input').val());
          // var bottle=parseFloat($('#edit-final_bottle-case-input').val());
          // var cap=parseFloat($('#edit-final_bottlecap_per-case-input').val());
          this.value=(total+liters).toFixed(4);
         });

          $(document).on('click','.delete',function(){
            var product=this.value;
            $.post("{% url 'costmanagement:deletefinal' %}",
            {
              'product':product,
            },
            function(data,status){
              // body...
            });
          });
	
	$(document).on('click','.approve',function(){
            var product=this.value;
            $.post("{% url 'costmanagement:approve_final_product' %}",
            {
              'sn':product,
	      'csrfmiddlewaretoken':'{{csrf_token}}',
            },
            function(data,status){
              window.location.reload();
            });
          });



        $(document).on('focusin','#final_qtylitrs-input',function(){
          var final=parseFloat($('#final_liters_perpack-input').val())*parseFloat($('#final_totalquantity-text-input').val());
          this.value=final.toFixed(4);
        });

        $(document).on('focusin','#edit-final_qtylitrs-input',function(){
          var final=parseFloat($('#edit-final_liters_perpack-input').val())*parseFloat($('#edit-final_totalquantity-text-input').val());
          this.value=final.toFixed(4);
        });

        $(document).on('focusin','#final_dkexPrice-input',function(){
          var total_str=$('#final_cartons-input').val();
          var total=parseFloat(total_str.substring(total_str.lastIndexOf('-')+1));
          var final=parseFloat($('#final_costpercasepail-input').val())+parseFloat($('#final_dkcost-input').val())+parseFloat($('#final_bottle-case-input').val())+parseFloat($('#final_label-case-input').val())+parseFloat($('#final_bottlecap_per-case-input').val())+total;
          this.value=final.toFixed(4);
        });

         $(document).on('focusin','#edit-final_dkexPrice-input',function(){
          var total_str=$('#edit-final_cartons-input').val();
          var total=parseFloat(total_str.substring(total_str.lastIndexOf('-')+1));
          var final=parseFloat($('#edit-final_costpercasepail-input').val())+parseFloat($('#edit-final_dkcost-input').val())+parseFloat($('#edit-final_bottle-case-input').val())+parseFloat($('#edit-final_label-case-input').val())+parseFloat($('#edit-final_bottlecap_per-case-input').val())+total;
          this.value=final.toFixed(4);
        });

            $("#finalproduct_date-input-date").datepicker({

                            }).on("changeDate", function(e) {
                               var d=new Date();
             d = e.date;
            d=(d.getDate() ) + '/' + (d.getMonth() + 1)+ '/' +  d.getFullYear();

                                $('#finalproduct_date-input').val(d);
                                // console.log(e.date);
                                // alert('hi');
                            });

                            $("#edit-finalproduct_date-input-date").datepicker({

                            }).on("changeDate", function(e) {
                               var d=new Date();
             d = e.date;
            d=(d.getDate() ) + '/' + (d.getMonth() + 1)+ '/' +  d.getFullYear();

                                $('#edit-finalproduct_date-input').val(d);
                                // console.log(e.date);
                                // alert('hi');
                            });


                            $(".table-search-product").on("keyup", function() {
  var value = $(this).val().toLowerCase(),
    tableattr = $(this).attr("data-table-search"),
    tablesearch = $('#' + tableattr).find('tbody tr');

  tablesearch.hide();                           //start with all rows hidden
                  //initiate our stored rowspan with the default of 1

  tablesearch.each(function() {
    var $row = $(this);
    var $firstCell = $row.find("td:nth-child(3)");
    var id = $firstCell.text().toLowerCase();
    if (id.indexOf(value) > -1) {               //if the text is found
      $row.show();         //show this row, and the next (n-1) rows as well
    }

  });
})
      });

    </script>
