			{% if user.authority == 'Manager 2' or user.authority == 'Manager 1'%}
                            <button type="button" id="{{i.sn}}" class="btn btn-primary primary edit" data-toggle="modal" data-target="#edit0" >Edit</button>
                        {% endif %}     
			 {% if user.authority == 'Manager 2'%}
                          <button type="button" value="{{i.sn}}" class="btn btn-danger delete">Delete</button>
			{% endif %}
				{% if user.authority == 'Manager 2' or user.authority == 'Accounts' %}
			<button value='{{i.sn}}' class="btn btn-primary approve">Approve</button>
				{% endif %}
                         </td>
                      </tr>

                      {% endfor %}
