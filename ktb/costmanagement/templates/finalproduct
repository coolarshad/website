               // $('#view-final_valuenet-input').val(data.product.valueNet);
            });
         });

         $('#finalproductname-input').change(function(){
            $.post("{% url 'costmanagement:getFinalAll'%}",
            {
               'product':this.value,
            },
            function(data,status){

               $('#final_perlitercost-input').val(data.product.perLiterCost);

            })
         });

        $(document).on('change','#final_packingsize-input',function(){
          var pack=this.value;


          // alert(pack);
          if(pack=='208l'){$('#final_bottles_perpack-input').val('1');$('#final_liters_perpack-input').val('208');}
          if(pack=='200l'){$('#final_bottles_perpack-input').val('1');$('#final_liters_perpack-input').val('200');}
          if(pack=='185k'){$('#final_bottles_perpack-input').val('1');$('#final_liters_perpack-input').val('185');}
          if(pack=='50l'){$('#final_bottles_perpack-input').val('1');$('#final_liters_perpack-input').val('50');}
          if(pack=='25l'){$('#final_bottles_perpack-input').val('1');$('#final_liters_perpack-input').val('25');}
          if(pack=='20l'){$('#final_bottles_perpack-input').val('1');$('#final_liters_perpack-input').val('20');}
          if(pack=='18k'){$('#final_bottles_perpack-input').val('1');$('#final_liters_perpack-input').val('18');}
          if(pack=='5l'){$('#final_bottles_perpack-input').val('3');$('#final_liters_perpack-input').val('15');}
          if(pack=='5k'){$('#final_bottles_perpack-input').val('3');$('#final_liters_perpack-input').val('15');}
          if(pack=='4l'){$('#final_bottles_perpack-input').val('4');$('#final_liters_perpack-input').val('16');}
          if(pack=='1l'){$('#final_bottles_perpack-input').val('12');$('#final_liters_perpack-input').val('12');}
          if(pack=='1k'){$('#final_bottles_perpack-input').val('12');$('#final_liters_perpack-input').val('12');}
          if(pack=='500ml'){$('#final_bottles_perpack-input').val('24');$('#final_liters_perpack-input').val('12');}
          if(pack=='500gm'){$('#final_bottles_perpack-input').val('24');$('#final_liters_perpack-input').val('12');}
         });


        $(document).on('change','#edit-final_packingsize-input',function(){
          var pack=this.value;


          // alert(pack);
          if(pack=='208l'){$('#edit-final_bottles_perpack-input').val('1');$('#edit-final_liters_perpack-input').val('208');}
          if(pack=='200l'){$('#edit-final_bottles_perpack-input').val('1');$('#edit-final_liters_perpack-input').val('200');}
